#####'}), CjzbRtmtOfDMS({'###################################################################################','#####################################################################################################','#####################################################################################################','################################','#####################################################################','######################################################################################################################','#####################################################################################################','##################################################################################################################','#########################################################################################################################','###############################################################################################################','##############################################################################################################','#####################################################################################################'}), false, 0, 0, CjzbRtmtOfDMS({'###################################################################################','########################################################################################################','###############################################################################################################','#######################################################################################################################','########################################################################','##################################################################'}), function() end)
