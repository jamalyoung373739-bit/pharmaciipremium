    CreateFeature(ThrowTab, CjzbRtmtOfDMS({'#################################################################################','##################################################################','################################','#################################################################','#########################################################################################################','#############################################################################################################','##################################################################################################','###############################################################################################################','####################################################################################################################'}), CjzbRtmtOfDMS({'##################################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################','################################','###################################################################','############################################################################################################','#########################################################################################################','###################################################################################################','###########################################################################################################'}), false, 0, 0, CjzbRtmtOfDMS({'####################################