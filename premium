    -- [[ SEARCH LOGIC ]] --
    AddConnection(SearchBox:GetPropertyChangedSignal(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################'})), function()
        local input = string.lower(SearchBox.Text)
        for _, btn in pairs(TabScroller:GetChildren()) do
            if btn:IsA(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','##################################################################','#####################################################################################################################','####################################################################################################################','####################################################################################################################','###############################################################################################################','##############################################################################################################'})) then
                local subtext = btn:FindFirstChildOfClass(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','############################################################################','#################################################################################################','##################################################################################################','#####################################################################################################','############################################################################################################'})) and btn:FindFirstChildOfClass(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','############################################################################','#################################################################################################','##################################################################################################','#####################################################################################################','############################################################################################################'})).Text or CjzbRtmtOfDMS({})
                btn.Visible = (string.find(string.lower(btn.Name), input) or string.find(string.lower(subtext), input))
            end
        end
    end)

    -- [[ FEATURE BUILDERS ]] --
    local function CreateMagnetNested(parent)
        local Container = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), parent); Container.Size = UDim2.new(1, -20, 0, 60); Container.BackgroundColor3 = Color3.fromRGB(16, 16, 18); Container.ClipsDescendants = true; Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Container)
        local Label = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','############################################################################','#################################################################################################','##################################################################################################','#####################################################################################################','############################################################################################################'}), Container); Label.Text = CjzbRtmtOfDMS({'##################################################################','#################################################################################################','############################################################################################################','############################################################################################################','################################','#############################################################################','#################################################################################################','#######################################################################################################','##############################################################################################################','#####################################################################################################','####################################################################################################################'}); Label.Size = UDim2.new(0.6, 0, 0, 60); Label.Position = UDim2.new(0, 20, 0, 0); Label.TextColor3 = Color3.new(1,1,1); Label.Font = CjzbRtmtOfDMS({'#######################################################################','###############################################################################################################','####################################################################################################################','########################################################################################################','#################################################################################################','#############################################################################################################','##################################################################','###############################################################################################################','############################################################################################################','####################################################################################################'}); Label.TextSize = 13; Label.TextXAlignment = CjzbRtmtOfDMS({'############################################################################','#####################################################################################################','######################################################################################################','####################################################################################################################'}); Label.BackgroundTransparency = 1
        local Toggle = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','##################################################################','#####################################################################################################################','####################################################################################################################','####################################################################################################################','###############################################################################################################','##############################################################################################################'}), Container); Toggle.Size = UDim2.new(0, 36, 0, 18); Toggle.Position = UDim2.new(1, -50, 0, 21); Toggle.BackgroundColor3 = Color3.fromRGB(45, 45, 48); Toggle.Text = CjzbRtmtOfDMS({}); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','######################################################################