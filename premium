        local Dot = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), Toggle); Dot.Size = UDim2.new(0, 14, 0, 14); Dot.Position = UDim2.new(0, 2, 0.5, -7); Dot.BackgroundColor3 = Color3.new(1,1,1); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Dot).CornerRadius = UDim.new(1, 0)
        local Dropdown = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), Container); Dropdown.Size = UDim2.new(1, -40, 0, 100); Dropdown.Position = UDim2.new(0, 20, 0, 60); Dropdown.BackgroundTransparency = 1
        local SLabel = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','############################################################################','#################################################################################################','##################################################################################################','#####################################################################################################','############################################################################################################'}), Dropdown); SLabel.Text = CjzbRtmtOfDMS({'###################################################################','#################################################################################################','####################################################################################################################','###################################################################################################','########################################################################################################','################################','##################################################################################','#################################################################################################','##############################################################################################################','#######################################################################################################','#####################################################################################################','##########################################################','################################'}) .. _G.MagRange; SLabel.Size = UDim2.new(1,0,0,15); SLabel.TextColor3 = Color3.fromRGB(150, 150, 155); SLabel.Font = CjzbRtmtOfDMS({'#######################################################################','###############################################################################################################','####################################################################################################################','########################################################################################################','#################################################################################################','#############################################################################################################'}); SLabel.TextSize = 11; SLabel.BackgroundTransparency = 1; SLabel.TextXAlignment = CjzbRtmtOfDMS({'############################################################################','#####################################################################################################','######################################################################################################','####################################################################################################################'})
        local Bg = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), Dropdown); Bg.Size = UDim2.new(1, 0, 0, 6); Bg.Position = UDim2.new(0, 0, 0, 25); Bg.BackgroundColor3 = Color3.fromRGB(40, 40, 42); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Bg)
        local Fill = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), Bg); Fill.Size = UDim2.new(_G.MagRange/50, 0, 1, 0); Fill.BackgroundColor3 = ACCENT_COLOR; Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Fill)
        local VisToggle = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','##################################################################','#####################################################################################################################','####################################################################################################################','####################################################################################################################','###############################################################################################################','##############################################################################################################'}), Dropdown); VisToggle.Size = UDim2.new(0, 30, 0, 15); VisToggle.Position = UDim2.new(1, -30, 0, 52); VisToggle.BackgroundColor3 = Color3.fromRGB(45, 45, 48); VisToggle.Text = CjzbRtmtOfDMS({}); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), VisToggle).CornerRadius = UDim.new(1, 0)
        local VisDot = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), VisToggle); VisDot.Size = UDim2.new(0, 11, 0, 11); VisDot.Position = UDim2.new(0, 2, 0.5, -5.5); VisDot.BackgroundColor3 = Color3.new(1,1,1); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), VisDot).CornerRadius = UDim.new(1, 0)
        local VisLbl = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','############################################################################','#################################################################################################','##################################################################################################','#####################################################################################################','############################################################################################################'}), Dropdown); VisLbl.Text = CjzbRtmtOfDMS({'###################################################################################','########################################################################################################','###############################################################################################################','#######################################################################################################################','################################','########################################################################','#########################################################################################################','####################################################################################################################','##################################################################################################','###############################################################################################################','########################################################################################################################','#####################################################################################################','###################################################################################################################'}); VisLbl.Size = UDim2.new(0, 100, 0, 15); VisLbl.Position = UDim2.new(1, -135, 0, 52); VisLbl.TextColor3 = Color3.fromRGB(150, 150, 155); VisLbl.Font = CjzbRtmtOfDMS({'#######################################################################','###############################################################################################################','####################################################################################################################','########################################################################################################','#################################################################################################','#############################################################################################################'}); VisLbl.TextSize = 11; VisLbl.BackgroundTransparency = 1; VisLbl.TextXAlignment = CjzbRtmtOfDMS({'##################################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################'})
        
        local function UpdateMag(val)
            MagnetEnabled = val
            ts:Create(Container, TweenInfo.new(0.3), {Size = UDim2.new(1, -20, 0, MagnetEnabled and 150 or 60)}):Play()
            ts:Create(Toggle, TweenInfo.new(0.2), {BackgroundColor3 = MagnetEnabled and ACCENT_COLOR or Color3.fromRGB(45, 45, 48)}):Play()
            ts:Create(Dot, TweenInfo.new(0.2), {Position = MagnetEnabled and UDim2.new(1, -17, 0.5, -7) or UDim2.new(0, 2, 0.5, -7)}):Play()
        end
        
        -- [[ MOBILE SLIDER SUPPORT ]] --
        AddConnection(Bg.InputBegan, function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                canDrag = false
                local move; move = AddConnection(uis.InputChanged, function(m)
                    if m.UserInputType == Enum.UserInputType.MouseMovement or m.UserInputType == Enum.UserInputType.Touch then
                        local pos = math.clamp((m.Position.X - Bg.AbsolutePosition.X) / Bg.AbsoluteSize.X, 0, 1)
                        Fill.Size = UDim2.new(pos, 0, 1, 0)
                        _G.MagRange = math.floor(pos * 50); SLabel.Text = CjzbRtmtOfDMS({'###################################################################','#################################################################################################','####################################################################################################################','###################################################################################################','########################################################################################################','################################','##################################################################################','#################################################################################################','##############################################################################################################','#######################################################################################################','#####################################################################################################','##########################################################','################################'}) .. _G.MagRange
                    end
                end)
                local endCon; endCon = AddConnection(uis.InputEnded, function(up) 
                    if up.UserInputType == Enum.UserInputType.MouseButton1 or up.UserInputType == Enum.UserInputType.Touch then 
                        move:Disconnect(); endCon:Disconnect(); canDrag = true 
                    end 
                end)
            end
        end)
        
        AddConnection(Toggle.MouseButton1Click, function() UpdateMag(not MagnetEnabled) end)
        AddConnection(VisToggle.MouseButton1Click, function()
            VisualsEnabled = not VisualsEnabled
            ts:Create(VisToggle, TweenInfo.new(0.2), {BackgroundColor3 = VisualsEnabled and ACCENT_COLOR or Color3.fromRGB(45, 45, 48)}):Play()
            ts:Create(VisDot, TweenInfo.new(0.2), {Position = VisualsEnabled and UDim2.new(1, -14, 0.5, -5.5) or UDim2.new(0, 2, 0.5, -5.5)}):Play()
        end)
        
        FeatureRegistry[CjzbRtmtOfDMS({'#############################################################################','#################################################################################################','#######################################################################################################','##############################################################################################################','#####################################################################################################','####################################################################################################################'})] = {
            SetState = function(s) if s ~= MagnetEnabled then UpdateMag(s) end end,
            GetState = function() return MagnetEnabled end,
            SetValue = function(v) _G.MagRange = v; SLabel.Text = CjzbRtmtOfDMS({'###################################################################','#################################################################################################','####################################################################################################################','###################################################################################################','########################################################################################################','################################','##################################################################################','#################################################################################################','##############################################################################################################','#######################################################################################################','#####################################################################################################','##########################################################','################################'})..v; Fill.Size = UDim2.new(v/50, 0, 1, 0) end,
            GetValue = function() return _G.MagRange end
        }
    end

    local function CreateButton(parent, title, callback)
        local Container = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), parent); Container.Size = UDim2.new(1, -20, 0, 50); Container.BackgroundColor3 = Color3.fromRGB(16, 16, 18); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Container)
        local Label = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','############################################################################','#################################################################################################','##################################################################################################','#####################################################################################################','############################################################################################################'}), Container); Label.Text = title; Label.Size = UDim2.new(0.6, 0, 1, 0); Label.Position = UDim2.new(0, 20, 0, 0); Label.TextColor3 = Color3.new(1,1,1); Label.Font = CjzbRtmtOfDMS({'#######################################################################','###############################################################################################################','####################################################################################################################','########################################################################################################','#################################################################################################','#############################################################################################################','##################################################################','###############################################################################################################','############################################################################################################','####################################################################################################'}); Label.TextSize = 13; Label.TextXAlignment = CjzbRtmtOfDMS({'############################################################################','#####################################################################################################','######################################################################################################','####################################################################################################################'}); Label.BackgroundTransparency = 1
        local Btn = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','##################################################################','#####################################################################################################################','####################################################################################################################','####################################################################################################################','###############################################################################################################','##############################################################################################################'}), Container); Btn.Size = UDim2.new(0, 100, 0, 30); Btn.Position = UDim2.new(1, -110, 0.5, -15); Btn.BackgroundColor3 = Color3.fromRGB(45, 45, 48); Btn.Text = CjzbRtmtOfDMS({'###################################################################','############################################################################################################','#########################################################################################################','###################################################################################################','###########################################################################################################'}); Btn.TextColor3 = Color3.new(1,1,1); Btn.Font = CjzbRtmtOfDMS({'#######################################################################','###############################################################################################################','####################################################################################################################','########################################################################################################','#################################################################################################','#############################################################################################################','##################################################################','###############################################################################################################','############################################################################################################','####################################################################################################'}); Btn.TextSize = 11; Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Btn)
        
        AddConnection(Btn.MouseButton1Click, function()
            ts:Create(Btn, TweenInfo.new(0.1), {BackgroundColor3 = ACCENT_COLOR}):Play()
            task.wait(0.1)
            ts:Create(Btn, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(45, 45, 48)}):Play()
            callback(Btn)
        end)
    end

    local function CreateFeature(parent, title, subText, hasSlider, defaultVal, maxVal, typeKey, callback)
        local Container = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), parent); Container.Size = UDim2.new(1, -20, 0, 60); Container.BackgroundColor3 = Color3.fromRGB(16, 16, 18); Container.ClipsDescendants = true; Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Container)
        local Label = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','############################################################################','#################################################################################################','##################################################################################################','#####################################################################################################','############################################################################################################'}), Container); Label.Text = title; Label.Size = UDim2.new(0.6, 0, 0, 60); Label.Position = UDim2.new(0, 20, 0, 0); Label.TextColor3 = Color3.new(1,1,1); Label.Font = CjzbRtmtOfDMS({'#######################################################################','###############################################################################################################','####################################################################################################################','########################################################################################################','#################################################################################################','#############################################################################################################','##################################################################','###############################################################################################################','############################################################################################################','####################################################################################################'}); Label.TextSize = 13; Label.TextXAlignment = CjzbRtmtOfDMS({'############################################################################','#####################################################################################################','######################################################################################################','####################################################################################################################'}); Label.BackgroundTransparency = 1
        local Toggle = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','##################################################################','#####################################################################################################################','####################################################################################################################','####################################################################################################################','###############################################################################################################','##############################################################################################################'}), Container); Toggle.Size = UDim2.new(0, 36, 0, 18); Toggle.Position = UDim2.new(1, -50, 0, 21); Toggle.BackgroundColor3 = Color3.fromRGB(45, 45, 48); Toggle.Text = CjzbRtmtOfDMS({}); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Toggle).CornerRadius = UDim.new(1, 0)
        local Dot = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), Toggle); Dot.Size = UDim2.new(0, 14, 0, 14); Dot.Position = UDim2.new(0, 2, 0.5, -7); Dot.BackgroundColor3 = Color3.new(1,1,1); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Dot).CornerRadius = UDim.new(1, 0)
        
        local CurrentValue = defaultVal 
        local SliderFunc = nil
        if hasSlider then
            local SliderFrame = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), Container); SliderFrame.Size = UDim2.new(1, -40, 0, 40); SliderFrame.Position = UDim2.new(0, 20, 0, 60); SliderFrame.BackgroundTransparency = 1
            local SLabel = Instance.new(CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################','########################################################################################################################','####################################################################################################################','############################################################################','#################################################################################################','##################################################################################################','#####################################################################################################','############################################################################################################'}), SliderFrame); SLabel.Text = subText .. CjzbRtmtOfDMS({'##########################################################','################################'}) .. defaultVal; SLabel.Size = UDim2.new(1,0,0,15); SLabel.TextColor3 = Color3.fromRGB(150, 150, 155); SLabel.Font = CjzbRtmtOfDMS({'#######################################################################','###############################################################################################################','####################################################################################################################','########################################################################################################','#################################################################################################','#############################################################################################################'}); SLabel.TextSize = 11; SLabel.BackgroundTransparency = 1; SLabel.TextXAlignment = CjzbRtmtOfDMS({'############################################################################','#####################################################################################################','######################################################################################################','####################################################################################################################'})
            local Bg = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), SliderFrame); Bg.Size = UDim2.new(1, 0, 0, 6); Bg.Position = UDim2.new(0, 0, 0, 25); Bg.BackgroundColor3 = Color3.fromRGB(40, 40, 42); Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Bg)
            local Fill = Instance.new(CjzbRtmtOfDMS({'######################################################################','##################################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), Bg); Fill.Size = UDim2.new(defaultVal/maxVal, 0, 1, 0); Fill.BackgroundColor3 = ACCENT_COLOR; Instance.new(CjzbRtmtOfDMS({'#####################################################################################','#########################################################################','###################################################################','###############################################################################################################','##################################################################################################################','##############################################################################################################','#####################################################################################################','##################################################################################################################'}), Fill)
            
            SliderFunc = function(val)
                CurrentValue = val 
                Fill.Size = UDim2.new(val/maxVal, 0, 1, 0)
                SLabel.Text = subText .. CjzbRtmtOfDMS({'##########################################################','################################'}) .. val
                
                if typeKey == CjzbRtmtOfDMS({'####################################################################################','#########################################################################################################','#############################################################################################################','#####################################################################################################'}) then 
                    _G.CustomTime = val
                    local feat = FeatureRegistry[typeKey]
                    if feat and feat.GetState() then game:GetService(CjzbRtmtOfDMS({'############################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################'})).ClockTime = val end
                elseif typeKey == CjzbRtmtOfDMS({'##################################################################','##################################################################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################'}) then
                    _G.CustomBright = val
                    local feat = FeatureRegistry[typeKey]
                    if feat and feat.GetState() then game:GetService(CjzbRtmtOfDMS({'############################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################'})).Brightness = val end
                elseif typeKey == CjzbRtmtOfDMS({'################################################################################','##################################################################################################################','#####################################################################################################','####################################################################################################','#########################################################################################################','###################################################################################################','####################################################################################################################'}) then 
                    _G.QBPrediction = val
                elseif typeKey == CjzbRtmtOfDMS({'########################################################################','##################################################################','#####################################################################'}) then 
                    _G.HBESize = val
                else
                    callback(val)
                end
            end

            -- [[ MOBILE SLIDER SUPPORT ]] --
            AddConnection(Bg.InputBegan, function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                    canDrag = false
                    local move; move = AddConnection(uis.InputChanged, function(m)
                        if m.UserInputType == Enum.UserInputType.MouseMovement or m.UserInputType == Enum.UserInputType.Touch then
                            local pos = math.clamp((m.Position.X - Bg.AbsolutePosition.X) / Bg.AbsoluteSize.X, 0, 1)
                            local val = math.floor(pos * maxVal)
                            SliderFunc(val)
                        end
                    end)
                    local endCon; endCon = AddConnection(uis.InputEnded, function(up) 
                        if up.UserInputType == Enum.UserInputType.MouseButton1 or up.UserInputType == Enum.UserInputType.Touch then 
                            move:Disconnect(); endCon:Disconnect(); canDrag = true 
                        end 
                    end)
                end
            end)
        end
        
        local active = false
        local function UpdateState(state)
            active = state
            
            -- Feature Toggle Logic
            if typeKey == CjzbRtmtOfDMS({'####################################################################################','#####################################################################################################################','###################################################################################################','###########################################################################################################'}) then AutoTuckEnabled = active
            elseif typeKey == CjzbRtmtOfDMS({'########################################################################','##################################################################','#####################################################################'}) then HBEEnabled = active 
            elseif typeKey == CjzbRtmtOfDMS({'##################################################################','############################################################################################################','###############################################################################################################','###################################################################################################','###########################################################################################################'}) then 
                AdvancedBlocking = active
                if not active then -- RESET HITBOXES
                    for _, player in pairs(game:GetService(CjzbRtmtOfDMS({'################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','###################################################################################################################'})):GetPlayers()) do
                        if player ~= p and player.Character then
                            local jersey = player.Character:FindFirstChild(CjzbRtmtOfDMS({'##########################################################################','#####################################################################################################','##################################################################################################################','###################################################################################################################','#####################################################################################################','#########################################################################################################################'})) or player.Character:FindFirstChild(CjzbRtmtOfDMS({'########################################################################','#########################################################################################################','####################################################################################################################','##################################################################################################','###############################################################################################################','########################################################################################################################'}))
                            if jersey then jersey.Size = Vector3.new(2.31, 5.4, 2.3) end
                        end
                    end
                end
            elseif typeKey == CjzbRtmtOfDMS({'#########################################################################','##############################################################################################################','######################################################################################################','##########################################################################','#####################################################################################################################','#############################################################################################################','################################################################################################################'}) then _G.InfiniteJump = active
            elseif typeKey == CjzbRtmtOfDMS({'##############################################################################','###############################################################################################################','###################################################################################################','############################################################################################################','#########################################################################################################','################################################################################################################'}) then 
                NoclipEnabled = active
                if not active and p.Character then 
                    local bodyParts = {CjzbRtmtOfDMS({'########################################################################','#####################################################################################################','#################################################################################################','####################################################################################################'}), CjzbRtmtOfDMS({'########################################################################','#####################################################################################################################','#############################################################################################################','#################################################################################################','##############################################################################################################','###############################################################################################################','#########################################################################################################','####################################################################################################','##################################################################################','###############################################################################################################','###############################################################################################################','####################################################################################################################','################################################################################','#################################################################################################','##################################################################################################################','####################################################################################################################'}), CjzbRtmtOfDMS({'#####################################################################################','################################################################################################################','################################################################################################################','#####################################################################################################','##################################################################################################################','####################################################################################','###############################################################################################################','##################################################################################################################','###################################################################################################################','###############################################################################################################'}), CjzbRtmtOfDMS({'############################################################################','###############################################################################################################','#######################################################################################################################','#####################################################################################################','##################################################################################################################','####################################################################################','###############################################################################################################','##################################################################################################################','###################################################################################################################','###############################################################################################################'}), CjzbRtmtOfDMS({'####################################################################################','###############################################################################################################','##################################################################################################################','###################################################################################################################','###############################################################################################################'})}
                    for _, name in pairs(bodyParts) do
                        local part = p.Character:FindFirstChild(name)
                        if part then part.CanCollide = true end
                    end
                end
            elseif typeKey == CjzbRtmtOfDMS({'###################################################################################','################################################################################################################','#####################################################################################################','#####################################################################################################','####################################################################################################'}) then 
                WalkspeedEnabled = active
                if not active and p.Character then
                    local h = p.Character:FindFirstChildOfClass(CjzbRtmtOfDMS({'########################################################################','#####################################################################################################################','#############################################################################################################','#################################################################################################','##############################################################################################################','###############################################################################################################','#########################################################################################################','####################################################################################################'}))
                    if h then h.WalkSpeed = 16 end
                end
            elseif typeKey == CjzbRtmtOfDMS({'################################################################################','###############################################################################################################','#######################################################################################################################','#####################################################################################################','##################################################################################################################'}) then 
                JumppowerEnabled = active
                if not active and p.Character then
                    local h = p.Character:FindFirstChildOfClass(CjzbRtmtOfDMS({'########################################################################','#####################################################################################################################','#############################################################################################################','#################################################################################################','##############################################################################################################','###############################################################################################################','#########################################################################################################','####################################################################################################'}))
                    if h then h.JumpPower = 50 end
                end
            elseif typeKey == CjzbRtmtOfDMS({'#################################################################################','##################################################################','######################################################################','###############################################################################################################','############################################################################################################','############################################################################################################','###############################################################################################################','#######################################################################################################################'}) then 
                QBFollowEnabled = active
                if not active then isFollowing = false end
            elseif typeKey == CjzbRtmtOfDMS({'####################################################################################','##################################################################################################################','#####################################################################################################################','###################################################################################################','###########################################################################################################'}) then 
                TruckingEnabled = active 
            elseif typeKey == CjzbRtmtOfDMS({'#################################################################','##############################################################################################################','####################################################################################################################','#########################################################################################################','###########################################################################','##############################################################################################################','###############################################################################################################','###################################################################################################','###########################################################################################################'}) then 
                AntiKnockbackEnabled = active
                if not active and p.Character then
                    local l = p.Character:FindFirstChild(CjzbRtmtOfDMS({'############################################################################','#####################################################################################################','######################################################################################################','####################################################################################################################','################################','#################################################################','##################################################################################################################','#############################################################################################################'})); local r = p.Character:FindFirstChild(CjzbRtmtOfDMS({'##################################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################','################################','#################################################################','##################################################################################################################','#############################################################################################################'}))
                    if l then l.Size = Vector3.new(1,2,1) end
                    if r then r.Size = Vector3.new(1,2,1) end
                end
            elseif typeKey == CjzbRtmtOfDMS({'##################################################################','#########################################################################################################','#######################################################################################################','########################################################################','#####################################################################################################','#################################################################################################','####################################################################################################'}) then 
                BigHeadEnabled = active
                if not active then
                    for _,v in pairs(game.Players:GetPlayers()) do
                        if v.Character and v.Character:FindFirstChild(CjzbRtmtOfDMS({'########################################################################','#####################################################################################################','#################################################################################################','####################################################################################################'})) then
                            v.Character.Head.Size = Vector3.new(2,1,1)
                            v.Character.Head.Transparency = 0
                        end
                    end
                end
            elseif typeKey == CjzbRtmtOfDMS({'###################################################################################','########################################################################################################','###############################################################################################################','#######################################################################################################################','########################################################################','##################################################################'}) then 
                ShowAllHitboxes = active
                if not active then
                    for _, player in pairs(game:GetService(CjzbRtmtOfDMS({'################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','###################################################################################################################'})):GetPlayers()) do
                        if player ~= p and player.Character then
                            local jersey = player.Character:FindFirstChild(CjzbRtmtOfDMS({'##########################################################################','#####################################################################################################','##################################################################################################################','###################################################################################################################','#####################################################################################################','#########################################################################################################################'})) or player.Character:FindFirstChild(CjzbRtmtOfDMS({'########################################################################','#########################################################################################################','####################################################################################################################','##################################################################################################','###############################################################################################################','########################################################################################################################'}))
                            if jersey then jersey.Transparency = 1 end
                        end
                    end
                end
            elseif typeKey == CjzbRtmtOfDMS({'#################################################################','#####################################################################################################################','####################################################################################################################','###############################################################################################################','####################################################################################','#################################################################################################','###################################################################################################','###########################################################################################################','############################################################################################################','#####################################################################################################'}) then AutoTackleEnabled = active
            elseif typeKey == CjzbRtmtOfDMS({'#################################################################################','##################################################################','#################################################################','#########################################################################################################','#############################################################################################################'}) then 
                QBAimbotEnabled = active
                if active then Notify(CjzbRtmtOfDMS({'#########################################################################','##############################################################################################################','######################################################################################################','###############################################################################################################'}), CjzbRtmtOfDMS({'########################################################################','###############################################################################################################','############################################################################################################','####################################################################################################','################################','##################################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################','################################','###################################################################','############################################################################################################','#########################################################################################################','###################################################################################################','###########################################################################################################','################################','####################################################################################################################','###############################################################################################################','################################','#################################################################','#########################################################################################################','#############################################################################################################'})) end
            elseif typeKey == CjzbRtmtOfDMS({'####################################################################################','#########################################################################################################','#############################################################################################################','#####################################################################################################'}) then 
                game:GetService(CjzbRtmtOfDMS({'############################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################'})).ClockTime = active and _G.CustomTime or OriginTime
            elseif typeKey == CjzbRtmtOfDMS({'##################################################################','##################################################################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################'}) then 
                game:GetService(CjzbRtmtOfDMS({'############################################################################','#########################################################################################################','#######################################################################################################','########################################################################################################','####################################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################'})).Brightness = active and _G.CustomBright or OriginBright
            end
            
            ts:Create(Container, TweenInfo.new(0.3), {Size = UDim2.new(1, -20, 0, (active and hasSlider) and 120 or 60)}):Play()
            ts:Create(Toggle, TweenInfo.new(0.2), {BackgroundColor3 = active and ACCENT_COLOR or Color3.fromRGB(45, 45, 48)}):Play()
            ts:Create(Dot, TweenInfo.new(0.2), {Position = active and UDim2.new(1, -17, 0.5, -7) or UDim2.new(0, 2, 0.5, -7)}):Play()
            if state then Notify(title, CjzbRtmtOfDMS({'#####################################################################','##############################################################################################################','#################################################################################################','##################################################################################################','############################################################################################################','#####################################################################################################','####################################################################################################'})) else Notify(title, CjzbRtmtOfDMS({'####################################################################','#########################################################################################################','###################################################################################################################','#################################################################################################','##################################################################################################','############################################################################################################','#####################################################################################################','####################################################################################################'})) end
        end
        
        AddConnection(Toggle.MouseButton1Click, function() UpdateState(not active) end)
        
        FeatureRegistry[typeKey] = {
            SetState = UpdateState,
            GetState = function() return active end,
            SetValue = SliderFunc,
            GetValue = function() return CurrentValue end 
        }
    end

    -- [[ CONFIGURATION SYSTEM ]] --
    local ConfigName = CjzbRtmtOfDMS({'################################################################################','########################################################################################################','#################################################################################################','##################################################################################################################','#############################################################################################################','#################################################################################################','###################################################################################################','#########################################################################################################','#########################################################################################################','###################################################################','###############################################################################################################','##############################################################################################################','######################################################################################################','#########################################################################################################','#######################################################################################################','##############################################','##########################################################################################################','###################################################################################################################','###############################################################################################################','##############################################################################################################'})

    local function SaveConfig()
        if not writefile then Notify(CjzbRtmtOfDMS({'#####################################################################','##################################################################################################################','##################################################################################################################','###############################################################################################################','##################################################################################################################'}), CjzbRtmtOfDMS({'#####################################################################','########################################################################################################################','#####################################################################################################','###################################################################################################','#####################################################################################################################','####################################################################################################################','###############################################################################################################','##################################################################################################################','################################','##############################################################################################################','###############################################################################################################','####################################################################################################################','################################','###################################################################################################################','#####################################################################################################################','################################################################################################################','################################################################################################################','###############################################################################################################','##################################################################################################################','####################################################################################################################','#####################################################################################################','####################################################################################################'})); return end
        
        local data = {}
        for key, feat in pairs(FeatureRegistry) do
            data[key] = {
                Enabled = feat.GetState(),
                Value = (feat.GetValue and feat.GetValue() or 0)
            }
        end
        writefile(ConfigName, http:JSONEncode(data))
        Notify(CjzbRtmtOfDMS({'###################################################################','###############################################################################################################','##############################################################################################################','######################################################################################################','#########################################################################################################','#######################################################################################################'}), CjzbRtmtOfDMS({'###################################################################################','#################################################################################################','######################################################################################################################','#####################################################################################################','####################################################################################################','################################','###################################################################################','#####################################################################################################################','###################################################################################################','###################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################','######################################################################################################','#####################################################################################################################','############################################################################################################','############################################################################################################','#########################################################################################################################'}))
    end

    local function LoadConfig()
        if not isfile or not readfile then Notify(CjzbRtmtOfDMS({'#####################################################################','##################################################################################################################','##################################################################################################################','###############################################################################################################','##################################################################################################################'}), CjzbRtmtOfDMS({'#####################################################################','########################################################################################################################','#####################################################################################################','###################################################################################################','#####################################################################################################################','####################################################################################################################','###############################################################################################################','##################################################################################################################','################################','##############################################################################################################','###############################################################################################################','####################################################################################################################','################################','###################################################################################################################','#####################################################################################################################','################################################################################################################','################################################################################################################','###############################################################################################################','##################################################################################################################','####################################################################################################################','#####################################################################################################','####################################################################################################'})); return end

        if isfile(ConfigName) then
            local data = http:JSONDecode(readfile(ConfigName))
            for key, setting in pairs(data) do
                if FeatureRegistry[key] then
                    FeatureRegistry[key].SetState(setting.Enabled)
                    if setting.Value and FeatureRegistry[key].SetValue then
                        FeatureRegistry[key].SetValue(setting.Value)
                    end
                end
            end
            Notify(CjzbRtmtOfDMS({'###################################################################','###############################################################################################################','##############################################################################################################','######################################################################################################','#########################################################################################################','#######################################################################################################'}), CjzbRtmtOfDMS({'############################################################################','###############################################################################################################','#################################################################################################','####################################################################################################','#####################################################################################################','####################################################################################################','################################','###################################################################################','#####################################################################################################################','###################################################################################################','###################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################','######################################################################################################','#####################################################################################################################','############################################################################################################','############################################################################################################','#########################################################################################################################'}))
        else
            Notify(CjzbRtmtOfDMS({'###################################################################','###############################################################################################################','##############################################################################################################','######################################################################################################','#########################################################################################################','#######################################################################################################'}), CjzbRtmtOfDMS({'##############################################################################','###############################################################################################################','################################','###################################################################################','#################################################################################################','######################################################################################################################','#####################################################################################################','################################','######################################################################','#########################################################################################################','############################################################################################################','#####################################################################################################','################################','######################################################################','###############################################################################################################','#####################################################################################################################','##############################################################################################################','####################################################################################################'}))
        end
    end

